{
  "name": "kal",
  "version": "0.0.0",
  "description": "Keyboard-driven app launcher and productivity tool.",
  "author": "Amr Bashir <amr.bashir2015@gmail.com>",
  "repository": "https://github.com/amrbashir/kal",
  "license": "MIT",
  "private": "true",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "ts:check": "vue-tsc --noEmit",
    "format": "prettier --write \"./**/*.{ts,tsx,css,scss,html,json,vue}\" --ignore-path ./.gitignore",
    "format:check": "prettier --check \"./**/*.{ts,tsx,css,scss,html,json,vue}\" --ignore-path ./.gitignore",
    "deps:up": "pnpm update --latest && cargo update"
  },
  "dependencies": {
    "vue": "3.3.12",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@types/node": "20.10.4",
    "@vitejs/plugin-vue": "4.5.2",
    "prettier": "3.1.1",
    "typescript": "5.3.3",
    "vite": "5.0.10",
    "vue-tsc": "1.8.25"
  }
}
